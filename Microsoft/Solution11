/*
Time Complexity -  O(N (log2 N))
Space Complexity - O(1)

Question Link- https://practice.geeksforgeeks.org/problems/generate-binary-numbers-1587115620/1/#
*/

vector<string> generate(int N)
{
    vector<string> vt;
    for(int x=1;x<=N;x++)
    {
        bitset<32> bt(x);
        int i=31;
        string st="";
        while(bt[i]==0)
            i--;
        while(i>=0)
        {
        
            st+= char(bt[i--]+48);
        }
        vt.push_back(st);
    }
    return vt;
}
